      "**/*.spec.ts",
      ".git",
      "node_modules",
      "cs-front-end"
    ],
    "watch": [
      "./"
    ],
    "exec": "npm run dev",
    "ext": "ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.21.0",
    "firebase-admin": "^9.1.1",
    "multer": "^1.4.2",
