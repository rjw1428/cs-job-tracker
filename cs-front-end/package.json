{
  "name": "wf-manager",
  "version": "1.0.0",
  "author": "Ryan Wilk",
  "description": "Workflow Manager",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:electron": "ng build --prod --base-href ./ && electron .",
    "electron": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "postinstall": "install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjw1428/cs-job-tracker"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.9.37",
    "@angular/animations": "~9.1.7",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.7",
    "@angular/compiler": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/forms": "~9.1.7",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.3.5",
    "file-saver": "^2.0.2",
    "rxjs": "~6.5.4",
    "sheetjs-style": "^0.15.6",
    "socket.io-client": "^2.3.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler-cli": "~9.1.7",
    "@ngrx/store-devtools": "^10.0.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/socket.io-client": "^1.4.33",
    "codelyzer": "^5.1.2",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "build": {
    "asar": false,
    "files": [
      "!src",
      "!e2e",
      "!packaged",
      "dist",
      "main.js",
      "LICENSE"
    ],
    "appId": "com.example.todo",
    "productName": "Workflow Manager",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "installerIcon": "src/assets/icon.ico",
      "uninstallerIcon": "src/assets/icon.ico",
      "license": "LICENSE",
      "runAfterFinish": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Workflow Manager"
    },
    "portable": {
      "requestExecutionLevel": "user"
    },
    "publish": {
      "provider": "github",
      "repo": "cs-job-tracker",
      "owner": "rjw1428"
    },
    "directories": {
      "output": "packaged"
    }
  }
}
